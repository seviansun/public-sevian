<script>
var send_code = false;
var modify_phone=false;
$(function(){
	$('.con-reg-btn').click(function() {
		isChecked();									 
	});
	$('.J-mobile-inp').blur(function(){
		var reg=/^13[0-9]{1}[0-9]{8}$|15[0189]{1}[0-9]{8}$|18[2689]{1}[0-9]{8}$/;						 
		var mobile=$(".J-mobile-inp").val();
		if(mobile!=""){	
			if(!reg.test(mobile)){
				wrongTips($('.modify-photo .Validform_checktip'),'该手机号码不存在');	
			}
			else{
				$(".J-mobile-inp").siblings(".Validform_checktip").removeClass("Validform_wrong").removeClass("Validform_right").text("");	
			}
		}else{
				wrongTips($('.modify-photo .Validform_checktip'),'手机号码不能为空');				
		}
	});	
	$(".send-code-btn").click(function(){
		var cellphone=$(".modify-photo .passport-input").val();	
        var action = 'register_reset_mobile';
		$.post("http://passport.liba.com/post.php", {action : action, mobile: cellphone},
		function(data){
			var errorMsg="";
			if(data=="s5"){
				$(".modify-photo").hide();
				$(".cellphone").text(cellphone);
				$(".code-write").show();
				$(".code-mana").show();
				modify_phone=true;
			}else{
				if(data=="e13"){
					errorMsg="手机格式错误";						
				}
				if(data=="e16"){
					errorMsg="手机号被禁止";						
				}
				if(data=="e17"){
					errorMsg="手机号用户数超过5个";						
				}
				if(data=="e24"){
					errorMsg="超过短信数";						
				}
				if(data=="e25"){
					errorMsg="超过注册短信数";						
				}
				if(data=="e21"){
					errorMsg="手机号不能为空";						
				}
				$('.modify-photo .Validform_checktip').removeClass("Validform_right").addClass('Validform_wrong');
				$('.modify-photo .Validform_checktip').html('<p>'+errorMsg+'</p><i class="sideline top"></i><i class="sideline bot"></i>');
				$('.modify-photo .Validform_checktip').show();
				$('.modify-photo').children(".validform-tips").hide();				
			}
		});
	});
	
	$('.agin-send').click(function() {
        var action = 'register_resend_sms';
		$.post("http://passport.liba.com/post.php", {action : action},
		   function(data) {
			if(data == "s4"){
				send();
			}					
		});
	});
	
	//60秒倒计时
	if($("#check-second").length > 0){
		send();
	}
	$(".input-code").blur(function(){
		checkCode($(".input-code"));
	});	
	$('.code-btn').click(function() {
		var valcode = $('.input-code').attr('value');
        var action = 'register_step_2';
		$.post("http://passport.liba.com/post.php", { valcode: valcode, action:action},
		   function(data) {
			var errorMsg="";
			if(data=="s3"||data=="e8"){
				$('.input-code').remove();
				$('.code-btn').remove();
				$('.code-mana').remove();
				$('.code-write .Validform_checktip').remove();
				$('.send-success').show();
				$(".show").hide();
				send_code=true;
			}else{
				if(data=="e26"){
					errorMsg="验证码输入有误";
				}
				if(data=="e27"){
					errorMsg="创建失败";
				}
				$('.code-write .Validform_checktip').html(errorMsg);
				wrongTips($('.code-write .Validform_checktip'),errorMsg);
			}
		});
	});
})
function isChecked(){
		var n= $(".interest-list").find("input:checked").length;
		if(send_code){
			if(n==0){
				Dialog.popTip("","请至少选择一个主题",2000);
			}else{
				$(".registerform").submit();}
		}else{
			$(".input-code").focus();
		}
		if(!modify_phone){
			$(".J-mobile-inp").focus();						
		}
}
</script>
<!--main end-->
